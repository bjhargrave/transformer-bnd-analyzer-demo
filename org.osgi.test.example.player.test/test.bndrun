-tester: biz.aQute.tester.junit-platform
-runee: JavaSE-1.8
-runvm: ${def;argLine}
-runtrace: true

-runfw: org.eclipse.osgi
-resolve.effective: active
-runproperties: \
	logback.configurationFile='${fileuri;${.}/logback.xml}',\
	org.apache.felix.http.host=localhost,\
	org.osgi.service.http.port=*,\
	org.osgi.framework.bootdelegation='sun.misc,sun.reflect',\
	osgi.console=

-runpath: \
	ch.qos.logback.classic,\
	ch.qos.logback.core,\
	org.apache.felix.logback,\
	slf4j.api,\
	jakarta.platform:jakarta.jakartaee-api;verion="[8.0,9.0)";strategy=lowest

-runsystempackages: javax.enterprise.concurrent, javax.inject, javax.annotation

-runrequires: \
	bnd.identity;id='${basename;${.}}',\
	bnd.identity;id='junit-jupiter-engine',\
	bnd.identity;id='junit-platform-launcher'
